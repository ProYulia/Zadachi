int [] array = new int[10];
FillArray(array);
foreach(int el in array)
{
    Console.WriteLine(el);
}

void FillArray(int[] nums)
{
    int length = nums.Length;
    int index = 0;
    while (index < length)
    { 
        nums[index] = new Random().Next(1, 10);
        index++; 
    }
}